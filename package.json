{
  "name": "Vinyl",
  "version": "1.0.0",
  "description": "A community for record enthusiasts to review their favorite albums.",
  "engines": {
    "node": "6.x"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "NODE_ENV=development nodemon src/server.js",
    "db:create": "createdb vinyl",
    "db:reset": "dropdb vinyl && createdb vinyl && npm run db:schema && npm run db:seed && npm run db:session",
    "db:schema": "psql -d vinyl < src/models/db/schema.sql",
    "db:seed": "psql -d vinyl < src/models/db/albums.sql",
    "db:session": "psql vinyl < node_modules/connect-pg-simple/table.sql",
    "test": "./node_modules/.bin/eslint src"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "connect-pg-simple": "^4.2.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.6",
    "immutable": "^3.8.2",
    "pg-promise": "^7.0.2",
    "pug": "^2.0.0-rc.4"
  },
  "devDependencies": {
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "nodemon": "^1.11.0"
  }
}
